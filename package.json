{
  "name": "purescript-manifold",
  "version": "1.0.0",
  "description": "Reactive state management for PureScript using Signal and Aff",
  "scripts": {
    "build": "pulp build --include lib:test || true",
    "build:babel": "babel src -s -d lib",
    "build:json": "pulp build --include lib:test --no-psa --json-errors || true",
    "postinstall": "bower install",
    "test": "pulp test || true",
    "test:watch": "pulp --watch --before clear test || true"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/purescript-manifold.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us>",
  "license": "MIT",
  "keywords": [
    "purescript",
    "state",
    "signal"
  ],
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-export-extensions": "^6.1.18",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.2.0",
    "eslint": "^2.11.1",
    "eslint-config-ecliptic": "^2.0.0",
    "eslint-plugin-react": "^5.1.1",
    "pulp": "^9.0.1",
    "purescript": "^0.9.1",
    "purescript-psa": "^0.3.9"
  }
}
